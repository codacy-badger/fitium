{
  "name": "fitium",
  "version": "0.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "babel-node --presets es2015 ./bin/www",
    "start:dev": "nodemon --exec babel-node ./bin/www --presets es2015",
